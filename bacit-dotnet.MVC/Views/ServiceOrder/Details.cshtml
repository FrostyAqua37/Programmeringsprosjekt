@model bacit_dotnet.MVC.Models.ServiceOrdre.Serviceform


@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>


    <h4>Serviceform</h4>
    @if (Model != null)
    {
        foreach (var item in Model.serviceform)
        {
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.OrderId)
                </dt>
                <dd class="col-sm-10">
                    @item.OrderId
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.FirstName)
                </dt>
                <dd class="col-sm-10">
                    @item.FirstName
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.LastName)
                </dt>
                <dd class="col-sm-10">
                    @item.LastName
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.PhoneNumber)
                </dt>
                <dd class="col-sm-10">
                    @item.PhoneNumber
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Email)
                </dt>
                <dd class="col-sm-10">
                    @item.Email
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.ProductName)
                </dt>
                <dd class="col-sm-10">
                    @item.ProductName
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.ProductType)
                </dt>
                <dd class="col-sm-10">
                    @item.ProductType
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.PreferredTimePeriod)
                </dt>
                <dd class="col-sm-10">
                    @item.PreferredTimePeriod
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Comment)
                </dt>
                <dd class="col-sm-10">
                    @item.Comment
                </dd>
            </dl>

        }
    }


<div>
    <a asp-action="Index">Back to List</a>
</div>


